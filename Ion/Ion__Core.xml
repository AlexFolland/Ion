	<Button name="IonPopupButtonTemplate" virtual="true">
		<CheckedTexture file="Interface\QuestFrame\UI-QuestTitleHighlight" alphaMode="ADD">
			<Anchors>
				<Anchor point="TOPLEFT">
					<Offset x="2" y="-2"/>
				</Anchor>
